{
  "expo": {
    "name": "deliveru",
    "slug": "deliveru",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "2",
      "supportsTablet": true,
      "bundleIdentifier": "io.kama.deliveru",
      "config": {
        "googleMapsApiKey": "YOUR MAPS API KEY"
      }
    },
    "android": {
      "versionCode": 3,
      "config": {
        "googleMaps": {
          "apiKey": "YOUR MAPS API KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mish_kama.deliveru"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8dcd5f02-d4d7-4f3e-9ede-3ed9aad08f84"
      }
    },
    "owner": "mish_kama",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 33,
            "targetSdkVersion": 33,
            "compileSdkVersion": 33,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "17.0"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ${PRODUCT_NAME} to use your location."
        }
      ]
    ]
  }
}
